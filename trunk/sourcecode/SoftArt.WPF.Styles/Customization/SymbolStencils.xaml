<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:SoftArt.WPF.Graph;assembly=SoftArt.WPF.Graph"
                    xmlns:c="clr-namespace:SoftArt.WPF.Graph.Controls;assembly=SoftArt.WPF.Graph"
                    xmlns:local="clr-namespace:SoftArt.WPF.Styles">

   <s:Toolbox x:Key="SymbolStencils">
        <s:Toolbox.Resources>
            <Style TargetType="{x:Type TextBlock}" x:Key="tbTextStyle">
                <Setter Property="Foreground" Value="Black"/>
                <Setter Property="FontSize" Value="10"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
            </Style>
            <Style TargetType="{x:Type Image}" x:Key="imageStyle">
                <Setter Property="IsHitTestVisible" Value="False"/>
                <Setter Property="Stretch" Value="Uniform"/>
            </Style>
            <Style TargetType="{x:Type Grid}" x:Key="gridStyle">
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
            </Style>
        </s:Toolbox.Resources>
    </s:Toolbox>
</ResourceDictionary>